{
  "name": "preybird-hospitality",
  "license": "PER",
  "version": "1.0.0",
  "description": "An app with hospitality",
  "scripts": {
    "build:ts": "tsc -p src/",
    "build:css": "node-sass src/ -o src/",
    "build:ng": "concurrently \"npm run build:ts\" \"npm run build:css\"",
    "watch-css": "concurrently \"npm run build:css\" \"node-sass src/ -o src/ --watch --recursive\"",
    "watch-ts": "tsc -p src/ -w",
    "build:watch": "npm-run-all -p watch-css watch-ts",
    "build:e2e": "tsc -p e2e/",
    "build:node": "uglifyjs src/app.js -c -m -o build/app.js",
    "serve:ng": "lite-server -c=bs-config.json",
    "serve:node": "node build/app.js",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build:ng",
    "start": "concurrently \"npm run serve:node\" \"npm run build:watch\" \"npm run serve:ng\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build:ng",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build:ng",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "Per Jonsson",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "body-parser": "1.15.1",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "cors": "2.7.1",
    "express": "4.13.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "moment": "2.13.0",
    "node-sass": "^4.5.2",
    "node-uuid": "1.4.7",
    "npm-run-all": "^4.0.2",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "~2.1.0",
    "uglify-js": "mishoo/UglifyJS2#harmony"
  },
  "repository": {}
}